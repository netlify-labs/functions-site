{
  "name": "netlify-functions",
  "description": "Netlify functions site",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "gatsby"
  ],
  "author": "David Wells <david.wells@netlify.com>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "prebuild": "cd functions/add-example && npm install",
    "build": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')"
  },
  "dependencies": {
    "analytics": "^0.1.2",
    "analytics-plugin-customerio": "^0.1.1",
    "analytics-plugin-google-tag-manager": "^0.1.0",
    "analytics-plugin-segment": "^0.1.1",
    "analytics-utils": "0.0.13",
    "classnames": "^2.2.6",
    "clipboard": "^1.7.1",
    "cssnano": "^4.1.10",
    "gatsby": "2.2.11",
    "gatsby-image": "2.0.34",
    "gatsby-plugin-analytics": "0.0.3",
    "gatsby-plugin-catch-links": "2.0.13",
    "gatsby-plugin-feed": "2.1.0",
    "gatsby-plugin-google-analytics": "2.0.17",
    "gatsby-plugin-lodash": "3.0.5",
    "gatsby-plugin-manifest": "2.0.24",
    "gatsby-plugin-nprogress": "2.0.10",
    "gatsby-plugin-offline": "2.0.25",
    "gatsby-plugin-react-helmet": "3.0.11",
    "gatsby-plugin-sharp": "2.0.31",
    "gatsby-plugin-sitemap": "2.0.10",
    "gatsby-plugin-twitter": "2.0.13",
    "gatsby-remark-autolink-headers": "2.0.16",
    "gatsby-remark-copy-linked-files": "2.0.11",
    "gatsby-remark-images": "3.0.10",
    "gatsby-remark-prismjs": "3.2.6",
    "gatsby-remark-responsive-iframe": "2.1.1",
    "gatsby-source-filesystem": "2.0.28",
    "gatsby-transformer-remark": "2.1.15",
    "gatsby-transformer-sharp": "2.1.17",
    "get-form-data": "^2.0.0",
    "icon-pipeline": "0.0.3",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "netlify-identity-widget": "^1.5.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-select": "^2.4.2",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.16.4",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylefmt": "^6.0.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "write-good": "^1.0.1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "eslintIgnore": [
    "public",
    "static",
    ".cache",
    "content"
  ],
  "esLintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true
        }
      ],
      "react/prefer-stateless-function": "off",
      "react/prop-types": "off",
      "react/no-danger": "off",
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "hrefLeft",
            "hrefRight",
            "to"
          ],
          "aspects": [
            "noHref",
            "invalidHref",
            "preferButton"
          ]
        }
      ]
    },
    "settings": {
      "import/core-modules": []
    },
    "env": {
      "browser": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 4
    }
  },
  "browserslist": [
    "Last 2 versions"
  ]
}
